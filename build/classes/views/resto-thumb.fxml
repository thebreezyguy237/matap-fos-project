<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="boxContainer" alignment="CENTER" onMouseClicked="#loadResto" prefHeight="197.0" prefWidth="188.0" style="-fx-border-color: #faebcd; -fx-border-width: 2; -fx-border-radius: 20;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RestoThumbController">
   <children>
      <ImageView fx:id="restoLogo" fitHeight="94.0" fitWidth="160.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
         <image>
            <Image url="@../resources/media/images/logo.png" />
         </image>
      </ImageView>
      <Label fx:id="restoName" text="African Food" />
   </children>
</VBox>
